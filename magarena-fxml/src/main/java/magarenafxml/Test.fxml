<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<VBox 
    alignment="TOP_CENTER" 
    prefHeight="242.0" 
    prefWidth="215.0" 
    xmlns="http://javafx.com/javafx/8" 
    xmlns:fx="http://javafx.com/fxml/1" 
    fx:controller="magarenafxml.TestController">
    <children>
        <Button 
            fx:id="button" 
            onAction="#handleButtonAction" 
            prefHeight="25.0" 
            prefWidth="124.0" 
            text="Click Me!" 
            textAlignment="CENTER" />
        <Label 
            fx:id="label" 
            minHeight="16.0" 
            minWidth="69.0" />
    </children>
    <padding>
        <Insets 
            bottom="10.0" 
            left="10.0" 
            right="10.0" 
            top="10.0" />
    </padding>
</VBox>
